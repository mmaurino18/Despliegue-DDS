{{#partial "contenidoP" }}
<main>
    <section class="form-login">

        <h5>Editar Entidad </h5>

        <form class="form-registro" method="post" action="/organismosDeControlP/{{organismo.getId}}/entidadesPrestadorasP/{{entidadPrestadora.getId}}/entidadesP/{{entidad.getId}}">
        <p1>Nombre Entidad:</p1>
        <input class="controls" type="text" name="nombre" placeholder="{{entidad.getNombre}}">

            <br>
            <p1> ------------------------------------------------------ </p1>
            <br>
            <p1> Localizacion Actual:</p1>
            <br>
            <p1> Tipo: Provincia </p1>
            <br>
            <p1> Buenos Aires </p1>
            <br>
            <p1> ------------------------------------------------------  </p1>
            <br>

            <button class="buttons" type="button"><a href="comunidad.html"> Editar Localizacion </a></button>
            <button class="buttons" type="submit"> Editar Entidad </button>
            <button class="buttons" type="button" onclick="volverAtras()"> Volver </button>
        </form>

        <script>
            function volverAtras() {
                window.history.back();
            }
        </script>

    </section>
</main>


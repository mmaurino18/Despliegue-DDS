{{#partial "contenidoP" }}
<div class="conteiner-tabla">
    <table>
        <thead>
        <tr>
            <th>Nombre Entidad</th>
            <th>Localizacion</th>
            <th>Establecimientos</th>
            <th><img src="../public/img/edit.svg" width="60" height="22" alt="Editar"></th>
            <th><img src="../public/img/delete.svg" width="60" height="22" alt="Eliminar"></th>
        </tr>
        </thead>
        <tbody>
        {{#each entidad}}
        <tr>
            <td>{{this.getNombre}}</td>
            <td>sin localizacion</td>
            <td><button class="buttons" type="submit"><a href="/organismosDeControlP/{{organismo.getId}}/entidadesPrestadorasP/{{entidadPrestadora.getId}}/entidadesP/{{this.getId}}/establecimientosP">Ver Establecimientos</a></button></td>
            <td><button class="buttons" type="submit"><a href="comunidad.html">Editar</a></button></td>
            <td><button class="buttons" type="submit"><a href="comunidad.html">Eliminar</a></button></td>
        </tr>
        {{/each}}
        </tbody>
    </table>
    <button class="buttonConfi" type="submit"><a href="configuracion.html">Agregar Entidad</a></button>
</div>
{{/partial}}
{{> templates/homePropietarioBase }}
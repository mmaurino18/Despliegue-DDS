{{#partial "contenidoP" }}
<div class="conteiner-tabla">
    <table>
        <thead>
        <tr>
            <th>Nombre Establecimiento</th>
            <th>Perteneciente a Entidad</th>
            <th>Descripcion</th>
            <th>Localizacion</th>
            <th>Servicios Prestados</th>
            <th><img src="/img/edit.svg" width="60" height="22"></th>
            <th><img src="/img/delete.svg" width="60" height="22"></th>
        </tr>
        </thead>
        <tbody>
        {{#each establecimiento}}
        <tr>
            <td>{{this.getNombre}}</td>
            <td>{{this.deEntidad}}</td>
            <td>{{this.getDescripcion}}</td>
            <td> sin localizacion </td>
            <td><button class="buttons" type="submit"><a href="comunidad.html">Ver Servicios Prestados</a></button></td>
            <td><button class="buttons" type="button"><a href="/entidadesP/{{entidad.getId}}/establecimientosP/{{this.getId}}/editar">Editar</a></button></td>
            <td><button class="buttons" type="submit"><a href="comunidad.html">Eliminar</a></button></td>
        </tr>
        {{/each}}
        </tbody>
    </table>
    <button class="buttonConfi" type="button"><a href="/entidadesP/{{entidad.getId}}/establecimientosP/crear">Agregar Establecimiento</a></button>
</div>
{{/partial}}
{{> templates/homePropietarioBase }}
{{#partial "contenidoP" }}

    {{#if vacio}}
        <div class="welcome-text">
            <p> no se cargo ningun Organismo de control</p>
        </div>
        <div>
            <button class="boton_estructura" type="button" onclick="volverAtras()"> Volver </button>
        </div>
    {{/if}}

    {{#if lleno}}
    <div class="conteiner-tabla">
        <table>
            <thead>
            <tr>
                <th>Nombre Organismo De Control</th>
                <th>Entidades Prestadoras</th>
                <th><img src="/img/edit.svg" width="60" height="22" alt="Editar"></th>
                <th><img src="/img/delete.svg" width="60" height="22" alt="Eliminar"></th>
            </tr>
            </thead>

            <tbody>
            {{#each organismoDeControl}}
               <tr>
                  <td>{{this.getNombre}}</td>
                  <td><button class="buttons" type="button"><a href="/entidadesPrestadorasP"> Ver Entidades Prestadoras</a></button></td>
                  <td><button class="buttons" type="button"><a href="/organismosDeControlP/{{this.getId}}/editar">Editar</a></button></td>
                  <td><button class="buttons" type="submit"><a href="comunidad.html">Eliminar</a></button></td>
               </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
    {{/if}}

    <script>
        function volverAtras() {
            window.history.back();
        }
    </script>

{{/partial}}
{{> templates/homePropietarioBase }}